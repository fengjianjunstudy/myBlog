<%- include ../include/header.ejs %>
<form class="form-horizontal">
    <div class="form-group">
        <label for="username" class="col-md-3 text-right">用户名</label>
        <div class="col-md-5">
            <input type="text" class="form-control " name="username" id="username" placeholder="用户名">
        </div>
    </div>
    <div class="form-group">
        <label for="pwd" class="col-md-3 text-right">密码</label>
        <div class="col-md-5">
            <input type="password" class="form-control" name="pwd" id="pwd" placeholder="Password">
        </div>

    </div>
    <div class="form-group">
        <label for="repwd" class="col-md-3 text-right">重复密码</label>
        <div class="col-md-5">
            <input type="password" class="form-control" name="repwd" id="repwd" placeholder="Password">
        </div>
    </div>
    <div class="col-md-offset-3 col-md-3">
        <button type="submit" class="btn btn-default ">Submit</button>
    </div>

</form>
<%- include ../include/footer.ejs %>

